void setup() {
  size(500, 400);
  
}

void draw() {
  if (mousePressed) {
    background(0, 0, 0);
    stroke(0);
    fill(255, 0, 0);
    rect(30,30,100,62);
    rect(370,30,100,62);
      fill(255, 255, 0);
    ellipse(250,170,100,62);
    
    fill(255, 255, 255);
  rect(100, 270, 300, 70);
  fill(0)
  rect(130, 280, 10, 50);
  rect(130,300,30,10);
  rect(160, 280, 10, 50);
  
  rect(190,280,10,50);
  rect(190,280,40,10);
  rect(190,320,40,10);
  rect(220,280,10,50);
  
  rect(260, 280, 10, 50);
  rect(260, 320, 40, 10);
  
  rect(320, 280, 10, 50);
  rect(320, 280, 40, 10);
  rect(355, 280, 10, 50);
  rect(320,305,40,10);
  
  
    
    if(mousePressed){
    fill(0,255,0);
    ellipse(mouseX,mouseY,50,50);
    }
  } 

else{
background(255, 255, 0);
  stroke(0);
  fill(0, 0, 255);
  ellipse(100, 100, 200, 200);
  fill(0,255,255);
  ellipse(mouseX-100,mouseY+30,30,30);
  
  fill(0, 0, 255);
  ellipse(400, 100, 200, 200);
  fill(0,255,255);
  ellipse(mouseX+100,mouseY+30,30,30);
  
  fill(0,150,0);
  rect(200,170,100,62);
  
 
  fill(255, 0, 0);
  rect(100, 270, 300, 70);
  fill(0)
  rect(110, 280, 10, 50);
  rect(110,300,30,10);
  rect(140, 280, 10, 50);
  
  rect(165, 280, 10, 50);
  rect(165, 280, 40, 10);
  rect(165, 300, 30, 10);
  rect(165, 320, 40, 10);
  
  rect(220, 280, 10, 50);
  rect(220, 320, 40, 10);
  
  rect(280, 280, 10, 50);
  rect(280, 320, 40, 10);
  
  rect(340,280,10,50);
  rect(340,280,40,10);
  rect(340,320,40,10);
  rect(370,280,10,50);

  fill(150,0,150); 
  
  
  }
}
